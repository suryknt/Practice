package Train;

import java.util.Scanner;

public class Substring_sum {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n=sc.nextInt();
		int prev=0;
		long result=0;
		int mod=1000000007;
		int count=10;
		int mult=1;
		while(n>0) {
			int curr=n%count;
			int sum=curr*mult+prev;
			result+=sum;
			result%=mod;
			n/=count;
			mult+=count;
			count*=10;
		}
		System.out.println(result);
	}
}
